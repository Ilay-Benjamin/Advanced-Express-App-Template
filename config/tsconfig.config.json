{
    "compilerOptions": {
        "module": "ES2020",
        "target": "ES2017",
        "rootDir": "./client",
        "outDir": "../public/config",
        "moduleResolution": "node",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "include": [
        "./client/**/*"
    ],
    "exclude": [
        "../node_modules",
      ],
    "ts-node": {
        "esm": true,
        "experimentalSpecifierResolution": "node"
    }
}